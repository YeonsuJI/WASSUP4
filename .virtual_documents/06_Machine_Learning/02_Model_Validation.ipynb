X_train, X_test, y_train, y_test = train_test_split(Elec[['surface_area']], 
                                                    Elec['electricity'], 
                                                    test_size = 0.2, 
                                                    random_state = 2045)


X_train.shape, y_train.shape, X_test.shape, y_test.shape 





X_train.head()





y_train.head()





sns.scatterplot(x = TR_Elec['surface_area'], y = TR_Elec['electricity'])
