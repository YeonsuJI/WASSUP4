    print(arr)


f = np.array( [1, 2, 3, 4, 5] )
g = np.array( ['A', 'B', 'C', 'D', 'E'] )
h = np.array( [True, True, False, False, True] )
#삼항연산            조건 ? 참 : 거짓
#                    참 if 조건 else 거짓  
print( [x if z else y for x, y, z in zip(f, g, h)] )

print( np.where( h ,f , g) )
print( np.where(f < 3, '작다', '크다') )
print( np.where( h ,'참' , '거짓') )


